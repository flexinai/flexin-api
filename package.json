{
  "name": "flexin-api",
  "version": "0.0.1",
  "description": "API for flexin.io",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources && tsc-alias",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "premigrate": "npm run build",
    "migrate": "node -r dotenv/config ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "predev": "npm run build",
    "dev": "node -r source-map-support/register -r dotenv/config ."
  },
  "repository": {
    "type": "git"
  },
  "author": "Tom Monks <monksy@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@aws-sdk/client-mediaconvert": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/s3-request-presigner": "^3.44.0",
    "@loopback/authentication": "^7.3.2",
    "@loopback/authentication-jwt": "^0.9.2",
    "@loopback/authentication-passport": "^3.2.2",
    "@loopback/authorization": "^0.9.2",
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-crud": "^0.11.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "@mailchimp/mailchimp_transactional": "^1.0.46",
    "auth0": "^2.37.0",
    "aws-sdk": "^2.1041.0",
    "axios": "^0.21.1",
    "dotenv": "^8.6.0",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.0.5",
    "loopback-connector-postgresql": "^5.2.1",
    "loopback-connector-rest": "^3.7.0",
    "passport": "^0.4.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.1",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/auth0": "^2.34.9",
    "@types/node": "^10.17.60",
    "@types/passport": "^1.0.4",
    "eslint": "^7.29.0",
    "source-map-support": "^0.5.19",
    "tsc-alias": "^1.7.0",
    "typescript": "~4.3.4"
  }
}
